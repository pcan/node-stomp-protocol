    "@types/node": "^10",
    "chai": "^4.1",
    "chai-as-promised": "^7.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2",
    "nyc": "^12.0",
    "source-map-support": "^0.5",
    "ts-node": "^3.2.0",
    "typescript": "^3",
    "@types/ws": "^5",
